<?xml version="1.0" encoding="UTF-8" ?>
<!--
  基础元数据myBatis文件
-->

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="BASIS_MD" >
  <!-- 数据定义:元数据模式 -->
  <resultMap id="MdModel" type="MdM" >
    <id     column="id"        property="id"        jdbcType="VARCHAR" />
    <result column="ownerId"   property="ownerId"   jdbcType="VARCHAR" />
    <result column="ownerType" property="ownerType" jdbcType="INTEGER" />
    <result column="tableName" property="tableName" jdbcType="VARCHAR" />
    <result column="cTime"     property="cTime"     jdbcType="TIMESTAMP" />
  </resultMap>
  <!-- 数据定义:列描述 -->
  <resultMap id="MdCol" type="MdColumn" >
    <id     column="id"          property="id"          jdbcType="VARCHAR" />
    <result column="tmId"        property="tmId"        jdbcType="VARCHAR" />
    <result column="columnName"  property="columnName"  jdbcType="VARCHAR" />
    <result column="titleName"   property="titleName"   jdbcType="VARCHAR" />
    <result column="columnType"  property="columnType"  jdbcType="VARCHAR" />
    <result column="columnIndex" property="columnIndex" jdbcType="INTEGER" />
    <result column="isPk"        property="isPk"        jdbcType="INTEGER" />
    <result column="cTime"       property="cTime"       jdbcType="TIMESTAMP" />
  </resultMap>
  <!-- 数据定义:列语义 -->
  <resultMap id="MdColS" type="MdColSemanteme" >
    <id     column="id"            property="id"            jdbcType="VARCHAR" />
    <result column="cId"           property="cId"           jdbcType="VARCHAR" />
    <result column="tmId"          property="tmId"          jdbcType="VARCHAR" />
    <result column="semantemeCode" property="semantemeCode" jdbcType="INTEGER" />
    <result column="semantemeType" property="semantemeType" jdbcType="INTEGER" />
    <result column="cTime"         property="cTime"         jdbcType="TIMESTAMP" />
    <result column="lmTime"        property="lmTime"        jdbcType="TIMESTAMP" />
  </resultMap>

  <!-- 元数据模式:获得所有 -->
  <select id="getAllMdMList" resultMap="MdModel" >
    select * from sa_md_tabModel
  </select>
  <!-- 元数据列描述模式:获得所有 -->
  <select id="getAllMdColList" resultMap="MdCol" >
    select * from sa_md_tabModel
  </select>
  <!-- 元数据列语义:获得所有 -->
  <select id="getAllMdColSemantemeList" resultMap="MdColS" >
    select * from sa_md_tabModel
  </select>
</mapper>